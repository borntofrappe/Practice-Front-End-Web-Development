themecolor = #E7C46E

*
    box-sizing border-box
    margin 0
    padding 0
  
body
    min-height 100vh
    width 100%
    background lighten(themecolor, 20%)

.container 
    // position the sprite in the middle of the page
    min-height 100vh
    display flex 
    justify-content center
    align-items center 

    .controller--toggle
        // position the toggle button in the top left of the page, always
        position fixed 
        top 10px
        left 10px
        width 72px
        height 72px
        border-radius 50%
        background themecolor
        filter drop-shadow(0 2px 5px darken(themecolor, 40%))
        border none
        // transition for the active state
        transition all 0.3s ease-out
        // display flex to center the nested svg asset
        display flex 
        align-items center
        justify-content center

        &:active
            // when pressed, increase the size of the button
            transform scale(1.1)

        svg 
            // stretch the svg to occupy 80% of the 72px wide button
            width 80%
            height auto 
            fill darken(themecolor, 40%)
            transform rotate(-20deg)

    .controller--pad
        // position the panel of controls always in the top left
        position fixed 
        top 100px
        left 25px
        width 100px
        height 100px
        // z-index to position the gamepad on top of potentially the sprite
        z-index 10
        // transition to smoothen the change in opacity and horizontal translation
        transition all 0.3s ease-out

        .pad--arrow
            // create 4 squares for the four arrows
            width 50px
            height 50px
            position absolute
            border 3px solid rgba(darken(themecolor, 40%), 0.4)
            background darken(themecolor, 20%) 
            // position the four buttons like a game controller
            &:nth-of-type(1)
                top 0
                left 50%
            &:nth-of-type(2)
                top 50%
                left 100%
            &:nth-of-type(3)
                top 100%
                left 50%
            &:nth-of-type(4)
                top 50%
                left 0

            &:active
                // increase the size when the buttons are pressed
                transform scale(1.05)

    .sprite
        // each sprite in the SVG graphic is 100px tall and 87.5px wide
        height 100px
        width 87.5px
        // include the svg as the background of the 87.5x100 div
        // write the url betweeen double quotes and place single quotes in the SVG declaration
        // otherwise stylus would compile the outer single quotes into double quotes and mess up the nested declaration
        // substitute # signs with %23 as the former is being deprecated as oart of the URI body
